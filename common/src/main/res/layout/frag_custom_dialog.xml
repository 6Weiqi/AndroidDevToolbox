<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="builder"
            type="com.liaobd.common.base.widget.CustomDialogFragCompat.Builder" />
    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ViewStub
            android:id="@+id/vs_dialog"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:inflatedId="@+id/vs_dialog_container"
            app:builder="@{builder}" />
    </FrameLayout>

</layout>